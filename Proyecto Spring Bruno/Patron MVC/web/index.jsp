    <body>
        <div class="container mt-4 col-lg-4">
            <div class="card col-sm-10">
                <div class="card-body">
                    <form class="form-sign" action="Coordinador" method="POST">
                        <div class="form-group text-center">
                            <h3>Login</h3>
                            <img src="img/user.png" alt="70" width="70"/>
                            <div>
                                <label>Bienvenido al Sistema</label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label>Usuario:</label>
                            <input type="text" name="txtuser" placeholder="Usuario" class="form-control" onkeypress="return soloLetras(event)" maxlength="20" autocomplete="off" required/>
                            <div class="invalid-feedback">
                                Campo requerido
                            </div>
                        </div>
                        <div class="form-group">
                            <label>Contraseña:</label>
                            <input placeholder="Contraseña" type="password" name="txtpass" maxlength="16" class="form-control" required/>
                            <div class="invalid-feedback">
                                Campo requerido
                            </div>
                        </div>
                        <input type="submit" name="accion" value="Ingresar" class="btn btn-primary btn-block">
                    </form>
                </div>
            </div>
        </div>
